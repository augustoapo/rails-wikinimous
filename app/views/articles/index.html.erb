<div class="container">
<h1 class = "d-flex justify-content-center">List of Articles</h1>


<ul class="list-group">
<% @articles.each do |article| %>
<h2><%= link_to article.title, article_path(article) %></h2>
<h3><%= article.content%>
<% end %>
</ul>
<br>
<p><%= link_to 'Create Article', new_article_path, class: "btn btn-primary" %></p>

</div>